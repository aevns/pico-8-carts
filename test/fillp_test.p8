frame = 0
fhalf = 0b1010010110100101.11
frand = fhalf

_set_fps(60)

function _update60()

end

function _draw()
	frame += 1
	cls()reset()
	if (frame % 5 == 0) then
		fb = rndbit()
		frand = frand ^^ fb
		fhalf = ~fhalf \1 | 0b0.11
	end
	
	circ(63,63,40,14)
	
	pal(t4)
	fillp()
	map(0, 0, 0, 0, 16, 16, 0)
	
	pal(t3)
	fillp(frand)
	map(1, 1, 8, 8, 14, 14, 0)
	fillp()
	map(2, 2, 16, 16, 12, 12, 0)
	
	pal(t2)
	fillp(frand)
	map(3, 3, 24, 24, 10, 10, 0)
	fillp()
	map(4, 4, 32, 32, 8, 8, 0)
	
	pal(t1)
	fillp(frand)
	map(5, 5, 40, 40, 6, 6, 0)
	fillp()
	map(6, 6, 48, 48, 4, 4, 0)
	
	pal(t4,2)
	pal(nil,1)
	
end
